<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Suite">

<groups>
<run>
<include name="smoke">
</include>
<exclude name= "regression">
</exclude>
</run>
</groups>
<listeners>
    <listener class-name="com.syntax.utils.Listeners"/>
</listeners>

 <test thread-count="5" name="Smoke Test">
   <packages>
     <package name="com.syntax.testcases"/>
    </packages>
 </test> <!-- Test -->
</suite> <!-- Suite -->